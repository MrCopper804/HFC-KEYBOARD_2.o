<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:keyHeight="60dp"
    android:horizontalGap="0dp"
    android:verticalGap="2dp">

    <Row>
        <Key
            android:codes="33"
            android:keyLabel="!"
            android:keyWidth="10%p" />
        <Key
            android:codes="64"
            android:keyLabel="@"
            android:keyWidth="10%p" />
        <Key
            android:codes="35"
            android:keyLabel="#"
            android:keyWidth="10%p" />
        <Key
            android:codes="36"
            android:keyLabel="$"
            android:keyWidth="10%p" />
        <Key
            android:codes="37"
            android:keyLabel="%"
            android:keyWidth="10%p" />
        <Key
            android:codes="94"
            android:keyLabel="^"
            android:keyWidth="10%p" />
        <Key
            android:codes="38"
            android:keyLabel="&amp;"
            android:keyWidth="10%p" />
        <Key
            android:codes="42"
            android:keyLabel="*"
            android:keyWidth="10%p" />
        <Key
            android:codes="40"
            android:keyLabel="("
            android:keyWidth="10%p" />
        <Key
            android:codes="41"
            android:keyLabel=")"
            android:keyWidth="10%p" />
    </Row>

    <Row>
        <Key
            android:codes="45"
            android:keyLabel="-"
            android:keyWidth="10%p" />
        <Key
            android:codes="95"
            android:keyLabel="_"
            android:keyWidth="10%p" />
        <Key
            android:codes="91"
            android:keyLabel="["
            android:keyWidth="10%p" />
        <Key
            android:codes="93"
            android:keyLabel="]"
            android:keyWidth="10%p" />
        <Key
            android:codes="123"
            android:keyLabel="{"
            android:keyWidth="10%p" />
        <Key
            android:codes="125"
            android:keyLabel="}"
            android:keyWidth="10%p" />
        <Key
            android:codes="124"
            android:keyLabel="|"
            android:keyWidth="10%p" />
        <Key
            android:codes="92"
            android:keyLabel="\\"
            android:keyWidth="10%p" />
        <Key
            android:codes="59"
            android:keyLabel=";"
            android:keyWidth="10%p" />
        <Key
            android:codes="-5"
            android:keyLabel="DEL"
            android:keyWidth="10%p"
            android:isModifier="true" />
    </Row>

    <Row>
        <Key
            android:codes="10001"
            android:keyLabel="ABC"
            android:keyWidth="25%p"
            android:isModifier="true" />
        <Key
            android:codes="58"
            android:keyLabel=":"
            android:keyWidth="10%p" />
        <Key
            android:codes="39"
            android:keyLabel="'"
            android:keyWidth="10%p" />
        <Key
            android:codes="34"
            android:keyLabel="\""
            android:keyWidth="10%p" />
        <Key
            android:codes="63"
            android:keyLabel="?"
            android:keyWidth="10%p" />
        <Key
            android:codes="47"
            android:keyLabel="/"
            android:keyWidth="10%p" />
    </Row>

    <Row>
        <Key
            android:codes="10000"
            android:keyLabel="123"
            android:keyWidth="25%p"
            android:isModifier="true" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="10002"
            android:keyLabel=" "
            android:keyWidth="30%p" />
        <Key
            android:codes="10003"
            android:keyLabel="ENTER"
            android:keyWidth="15%p"
            android:isModifier="true" />
    </Row>

</Keyboard>
